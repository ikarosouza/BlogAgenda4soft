<h1>Contatos</h1>
 
<table>
  <tr>
    <th>#ID</th>
    <th>Nome</th>
  </tr>
 
  <% @contacts.each do |contact| %>
    <tr>
      <td>#<%= contact.id %></td>
      <td><%= contact.name %></td>
      <td><%= link_to 'Ver', contact_path(contact) %></td>
    </tr>
  <% end %>
</table>

</p>
